.shake
	shake()

@keyframes shake
	0%
		transform: translate(3px, 0)
	50%
		transform: translate(-3px, 0)
	100%
		transform: translate(0, 0)

.pulse
	pulse()

@keyframes pulse
	0%
		opacity 1.0
		transform scale(1.0)
	50%
		opacity 0.75
		transform scale(1.1)
	// 67%
	// 	opacity 0.5
	// 	transform scale(1.0)
	100%
		opacity 1.0
		transform scale(1.0)

